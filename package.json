    "start": "node .",
    "pretest": "jest --clearCache",
    "test": "jest",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prebuild": "rimraf ./dist",
    "build": "babel ./src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "watch-ts": "babel ./src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --watch",
    "dev": "rm -rf ./node_modules/.cache/babel-loader && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"blue.bold,green.bold\" \"npm run watch-ts\" \"npm run nodemon\"",
    "nodemon": "nodemon --delay 1500ms -e js --watch dist .",
    "deploy": "cd dist && git init && git add . && git commit -am 'test' && git push heroku master"
  },
